@font-face {
  font-family: 'octicons';
  src: url('<%= font_path("octicons.eot") %>?#iefix') format('embedded-opentype'),
       url('<%= font_path("octicons.woff") %>') format('woff'),
       url('<%= font_path("octicons.ttf") %>') format('truetype'),
       url('<%= font_path("octicons.svg") %>#octicons') format('svg');
  font-weight: normal;
  font-style: normal;
}
